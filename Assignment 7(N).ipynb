   "source": [
    "fhandle = open(\"mbox.txt\",\"r\")\n",
    "email = dict()\n",
    "for a in fhandle:\n",
    "    if a.startswith(\"From:\") or a.startswith(\"To:\"):\n",
    "        b = a.split()\n",
    "        email[b[1]] = email.get(b[1],0) + 1\n",
    "for c,d in email.items():\n",
    "        print(\"Email: \",c)\n",
    "        print(\"Occurences: \",d)\n",
    "        print(\"\")"
   ]
  }
